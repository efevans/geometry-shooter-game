config_version=5

[application]

config/name="geometry-shooter-game"
config/description="Shoot the geometry shapes!"
run/main_scene="res://scenes/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.png"
config/windows_native_icon="res://favicon.ico"

[autoload]

SettingsManager="*res://scenes/autoload/settings_manager/settings_manager.tscn"
MusicManager="*res://scenes/autoload/music_manager/music_manager.tscn"
GameEvents="*res://scenes/autoload/game_events/game_events.tscn"
HighScoreManager="*res://scenes/autoload/high_score_manager/high_score_manager.tscn"
CommonObjects="*res://scenes/autoload/common_objects/common_objects.tscn"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=450
window/size/viewport_height=800
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[editor]

movie_writer/movie_file="res://"

